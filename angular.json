{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "netgrif-application-engine": {
            "projectType": "library",
            "root": "projects/netgrif-application-engine",
            "sourceRoot": "projects/netgrif-application-engine/src",
            "prefix": "nae",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-ng-packagr:build",
                    "options": {
                        "tsConfig": "projects/netgrif-application-engine/tsconfig.lib.json",
                        "project": "projects/netgrif-application-engine/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "projects/netgrif-application-engine/tsconfig.lib.prod.json"
                        }
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/netgrif-application-engine/src/test.ts",
                        "tsConfig": "projects/netgrif-application-engine/tsconfig.spec.json",
                        "karmaConfig": "projects/netgrif-application-engine/karma.conf.js",
                        "codeCoverage": true
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "projects/netgrif-application-engine/tsconfig.lib.json",
                            "projects/netgrif-application-engine/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        },
        "nae-example-app": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "projects/nae-example-app",
            "sourceRoot": "projects/nae-example-app/src",
            "prefix": "nae-app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/nae-example-app",
                        "index": "projects/nae-example-app/src/index.html",
                        "main": "projects/nae-example-app/src/main.ts",
                        "polyfills": "projects/nae-example-app/src/polyfills.ts",
                        "tsConfig": "projects/nae-example-app/tsconfig.app.json",
                        "aot": true,
                        "assets": [
                            "projects/nae-example-app/src/favicon.ico",
                            "projects/nae-example-app/src/assets"
                        ],
                        "styles": [
                            "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
                            "projects/nae-example-app/src/styles.scss"
                        ],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "projects/nae-example-app/src/environments/environment.ts",
                                    "with": "projects/nae-example-app/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "nae-example-app:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "nae-example-app:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "nae-example-app:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/nae-example-app/src/test.ts",
                        "polyfills": "projects/nae-example-app/src/polyfills.ts",
                        "tsConfig": "projects/nae-example-app/tsconfig.spec.json",
                        "karmaConfig": "projects/nae-example-app/karma.conf.js",
                        "assets": [
                            "projects/nae-example-app/src/favicon.ico",
                            "projects/nae-example-app/src/assets"
                        ],
                        "styles": [
                            "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
                            "projects/nae-example-app/src/styles.scss"
                        ],
                        "scripts": []
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "projects/nae-example-app/tsconfig.app.json",
                            "projects/nae-example-app/tsconfig.spec.json",
                            "projects/nae-example-app/e2e/tsconfig.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                },
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "projects/nae-example-app/e2e/protractor.conf.js",
                        "devServerTarget": "nae-example-app:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "nae-example-app:serve:production"
                        }
                    }
                }
            }
        }
    },
    "defaultProject": "nae-example-app"
}
