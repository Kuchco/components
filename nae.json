        "dashboard": {
            "layout": {
                "name": "dashboard",
                "params": {
                    "columns": 4,
                    "cards": [
                        {
                            "type": "count",
                            "title": "All tasks",
                            "resourceType": "Task",
                            "filter": {},
                            "layout": {
                                "x": 0,
                                "y": 0,
                                "rows": 1,
                                "cols": 1
                            }
                        },
                        {
                            "type": "iframe",
                            "url": "https://netgrif.com/",
                            "layout": {
                                "x": 2,
                                "y": 0,
                                "rows": 2,
                                "cols": 2
                            }
                        },
                        {
                            "type": "count",
                            "title": "All cases",
                            "resourceType": "Case",
                            "filter": {},
                            "layout": {
                                "x": 1,
                                "y": 1,
                                "rows": 1,
                                "cols": 1
                            }
                        }
                    ]
                },
                "componentName": "MyDashboard"
