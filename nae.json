                    "access": "private",
                    "navigation": {
                        "icon": "account_circle"
                    },
                    "children": {
                        "some_specifics": {
                            "layout": {
                                "name": "emptyView"
                            },
                            "access": "private",
                            "navigation": true,
                            "routing": {
                                "path": "comp-some_specifics"
                            }
                        }
                    },
                    "routing": {
                        "path": "comp-some_cases"
                    }
                }
            },
            "routing": {
                "path": "comp-cases"
