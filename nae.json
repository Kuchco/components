                "class": "CaseViewComponent",
                "from": "./doc/case-view/case-view.component"
            },
            "navigation": false,
            "access": "private",
            "routing": {
                "path": "comp-custom"
            }
        },
        "tab": {
            "layout": {
                "name": "tabView",
                "componentName": "MyTabs",
                "params": {
                    "tabs": [
                        {
                            "view": {
                                "name": "login",
                                "componentName": "TabbedLogin"
                            },
                            "canBeClosed": false
