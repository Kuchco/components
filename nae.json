            "layout": {
                "name": "caseView",
                "params": {
                    "allowedNets": [
                        "search-test",
                        "search-test-defaultRole",
                        "leukemia",
                        "1",
                        "bill",
                        "drobny_nakup",
                        "new_model",
                        "processId1"
