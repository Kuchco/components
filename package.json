{
    "name": "@netgrif/application-engine-project",
    "version": "4.1.0-alpha.9",
    "description": "Netgrif Application Engine Frontend project. Project includes angular library as base for NAE applications.",
    "homepage": "https://netgrif.com",
    "repository": {
        "type": "git",
        "url": "https://bitbucket.org/netgrif/nae-frontend.git"
    },
    "bugs": {
        "url": "https://netgrif.atlassian.net/browse/NAE"
    },
    "keywords": [
        "netgrif",
        "application engine",
        "nae",
        "engine",
        "workflow engine"
    ],
    "contributors": [
        "Milan Mladoniczky <mladoniczky@netgrif.com>",
        "Juraj Mažári <mazari@netgrif.com>",
        "Tomáš Kováčik <kovacik@netgrif.com>",
        "Jakub Kovář <kovar@netgrif.com>",
        "Martin Miklovič <miklovic@netgrif.com>",
        "Daniel Vaník <vanik@netgrif.com>",
        "Lukáš Rezák <rezak@netgrif.com>"
    ],
    "scripts": {
        "ng": "ng",
        "nae:build": "ng build netgrif-application-engine --prod && npm run nae:build-schematics",
        "nae:build-schematics": "node projects/netgrif-application-engine/src/scripts/build-schematics.js",
        "nae:full-test": "ng lint netgrif-application-engine && ng test netgrif-application-engine",
        "nae:normalize-coverage-summary": "node projects/netgrif-application-engine/src/scripts/normalize-file-paths.js coverage/netgrif-application-engine/coverage-summary.json",
        "nae:typedoc": "typedoc --tsconfig projects/netgrif-application-engine/tsconfig.lib.json  projects/netgrif-application-engine/src/lib",
        "nae:doc": "npm run nae:normalize-coverage-summary && compodoc",
        "nae:sonar": "node projects/netgrif-application-engine/src/scripts/sonar-scanner-analysis.js",
        "nae:local-build": "npm run nae:build && npm i dist/netgrif-application-engine --save-optional",
        "nae:prod-build": "npm run nae:full-test && npm run nae:sonar && npm run nae:build && npm run nae:doc",
        "nae:prod-build-without-tests": "ng lint netgrif-application-engine && npm run nae:sonar && npm run nae:build && npm run nae:doc",
        "project:start": "npm run nae:local-build && ng serve",
        "example:start": "ng serve",
        "example:build": "ng build --prod --base-href /projects/engine-frontend/examples/",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e"
    },
    "private": true,
    "dependencies": {
        "@angular-material-components/datetime-picker": "^2.0.2",
        "@angular-material-components/moment-adapter": "^2.0.1",
        "@angular/animations": "^9.0.5",
        "@angular/cdk": "~8.2.3",
        "@angular/common": "^9.0.5",
        "@angular/compiler": "^9.0.5",
        "@angular/core": "^9.0.5",
        "@angular/flex-layout": "^9.0.0-beta.29",
        "@angular/forms": "^9.0.5",
        "@angular/material": "~8.2.3",
        "@angular/material-moment-adapter": "^9.2.0",
        "@angular/platform-browser": "^9.0.5",
        "@angular/platform-browser-dynamic": "^9.0.5",
        "@angular/router": "^9.0.5",
        "@covalent/core": "^3.0.0",
        "@covalent/highlight": "^3.0.0",
        "@covalent/markdown": "^3.0.0",
        "@covalent/text-editor": "^3.0.1",
        "@ngx-translate/core": "^12.1.2",
        "@ngx-translate/http-loader": "^4.0.0",
        "@schematics/angular": "^9.0.5",
        "angular-resize-event": "^1.2.1",
        "angular2-hotkeys": "^2.2.0",
        "hammerjs": "^2.0.8",
        "json-schema-to-typescript": "^8.1.0",
        "jszip": "^3.2.2",
        "moment": "~2.24.0",
        "natural-orderby": "^2.0.3",
        "palette-creator": "^0.5.4",
        "rxjs": "^6.5.4",
        "tslib": "^1.11.1",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.900.5",
        "@angular-devkit/build-ng-packagr": "^0.900.5",
        "@angular-devkit/schematics-cli": "^0.901.0",
        "@angular/cli": "^9.0.5",
        "@angular/compiler-cli": "^9.0.5",
        "@angular/language-service": "^9.0.5",
        "@compodoc/compodoc": "^1.1.11",
        "@types/jasmine": "^3.5.9",
        "@types/jasminewd2": "~2.0.3",
        "@types/moment": "^2.13.0",
        "@types/node": "^12.12.29",
        "codelyzer": "^5.2.1",
        "commander": "^4.1.1",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~4.2.1",
        "json-server": "^0.15.1",
        "karma": "~4.3.0",
        "karma-chrome-launcher": "^3.1.0",
        "karma-coverage-istanbul-reporter": "~2.1.0",
        "karma-firefox-launcher": "^1.3.0",
        "karma-jasmine": "~2.0.1",
        "karma-jasmine-html-reporter": "^1.5.2",
        "karma-junit-reporter": "^2.0.1",
        "karma-mocha-reporter": "^2.2.5",
        "karma-nyan-reporter": "^0.2.5",
        "ncp": "^2.0.0",
        "ng-packagr": "^9.0.3",
        "protractor": "^5.4.3",
        "puppeteer": "^2.1.1",
        "schematics-utilities": "^2.0.1",
        "sonarqube-scanner": "^2.5.0",
        "ts-node": "~8.3.0",
        "tslint": "~5.18.0",
        "typescript": "~3.6.5",
        "typescript-json-schema": "^0.42.0",
        "unique-names-generator": "^4.2.0"
    },
    "optionalDependencies": {
        "@netgrif/application-engine": "file:dist/netgrif-application-engine"
    }
}
