{
    "name": "@netgrif/application-engine-project",
    "version": "4.0.0",
    "scripts": {
        "ng": "ng",
        "nae:build": "ng build netgrif-application-engine --prod",
        "nae:test": "ng lint netgrif-application-engine && ng test netgrif-application-engine",
        "nae:typedoc": "typedoc --tsconfig projects/netgrif-application-engine/tsconfig.lib.json  projects/netgrif-application-engine/src/lib",
        "nae:doc": "compodoc -p projects/netgrif-application-engine/tsconfig.lib.json -d docs/compodoc --theme gitbook --customLogo assets/icon-512x512.png",
        "nae:local-build": "npm run nae:build && cd projects/netgrif-application-engine && npm run build && cd ../.. && npm i dist/netgrif-application-engine --save-optional",
        "nae:prod-build": "npm run nae:test && npm run nae:build && npm run nae:doc",
        "project:start": "npm run nae:build && ng serve",
        "example-app:start": "ng serve",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^9.0.0-rc.8",
        "@angular/cdk": "~8.2.3",
        "@angular/common": "^9.0.0-rc.8",
        "@angular/compiler": "^9.0.0-rc.8",
        "@angular/core": "^9.0.0-rc.8",
        "@angular/flex-layout": "^9.0.0-beta.29",
        "@angular/forms": "^9.0.0-rc.8",
        "@angular/material": "~8.2.3",
        "@angular/platform-browser": "^9.0.0-rc.8",
        "@angular/platform-browser-dynamic": "^9.0.0-rc.8",
        "@angular/router": "^9.0.0-rc.8",
        "@covalent/core": "^2.1.1",
        "@covalent/highlight": "^2.1.1",
        "@covalent/markdown": "^2.1.1",
        "@schematics/angular": "^9.0.1",
        "hammerjs": "^2.0.8",
        "json-schema-to-typescript": "^8.0.1",
        "natural-orderby": "^2.0.3",
        "rxjs": "^6.5.4",
        "tslib": "^1.10.0",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.900.0-rc.8",
        "@angular-devkit/build-ng-packagr": "^0.900.0-rc.8",
        "@angular-devkit/schematics-cli": "^0.900.1",
        "@angular/cli": "^9.0.0-rc.8",
        "@angular/compiler-cli": "^9.0.0-rc.8",
        "@angular/language-service": "^9.0.0-rc.8",
        "@compodoc/compodoc": "^1.1.11",
        "@types/jasmine": "~3.5.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^12.12.24",
        "codelyzer": "^5.2.1",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~4.3.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~2.1.0",
        "karma-jasmine": "~2.0.1",
        "karma-jasmine-html-reporter": "^1.5.1",
        "karma-mocha-reporter": "^2.2.5",
        "ng-packagr": "^9.0.0-rc.7",
        "protractor": "~5.4.2",
        "schematics-utilities": "^2.0.1",
        "ts-node": "~8.3.0",
        "tslint": "~5.18.0",
        "tslint-strict-null-checks": "^1.0.1",
        "typedoc": "^0.16.6",
        "typescript": "~3.6.4"
    },
    "optionalDependencies": {
        "@netgrif/application-engine": "file:dist/netgrif-application-engine"
    }
}
