{
    "$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
    "dest": "../../dist/netgrif-application-engine",
    "assets": [
        "./src/assets",
        "**/*theme.scss",
        "./nae-typography.scss"
    ],
    "lib": {
        "entryFile": "src/public-api.ts",
        "umdModuleIds": {
