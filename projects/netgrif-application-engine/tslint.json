{
    "extends": "../../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "nae",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "nae",
            "kebab-case"
        ],
        "no-console": false,
        "variable-name": [
            "ban-keywords",
            "check-format",
            "allow-leading-underscore",
            "allow-pascal-case"
        ],
        "no-string-literal": false
    }
}
