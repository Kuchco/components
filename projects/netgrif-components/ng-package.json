{
    "$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
    "dest": "../../dist/netgrif-components",
    "assets": [
        "./src/assets",
        "**/*theme.scss",
        "./nae-typography.scss"
    ],
    "lib": {
        "entryFile": "src/public-api.ts",
        "umdModuleIds": {
            "@netgrif/components": "@netgrif/components",
            "@netgrif/components-core": "@netgrif/components-core",
            "jszip": "JSZip",
            "angular-resize-event": "angularResizeEvent",
            "@covalent/core": "@covalent/core"
        }
    },
    "whitelistedNonPeerDependencies": [
        "@netgrif/components-core"
    ]
}
